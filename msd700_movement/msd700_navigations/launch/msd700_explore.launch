<launch>
    <!-- Arguments -->
    <arg name="move_forward_only"   default="false"/>
    <arg name="use_simulator"       default="false"/>
    <arg name="open_rviz"           default="false"/>

    <include file="$(find msd700_launch)/launch/bridger.launch"/>

    <!-- msd700_slam -->
    <include file="$(find msd700_navigation)/launch/msd700_slam.launch">
        <arg name="rviz_config" value="$(arg rviz_config)"/>
    </include>
    
    <!-- move_base -->
    <include file="$(find msd700_launch)/launch/move_base.launch">
        <arg name="move_forward_only" value="$(arg move_forward_only)"/>
    </include>

    <!-- exploration -->
    <include file="$(find explore_lite)/launch/explore.launch" />
</launch>
  
